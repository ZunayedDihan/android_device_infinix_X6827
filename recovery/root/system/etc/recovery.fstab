# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#TODO: Add 'check' as fs_mgr_flags with data partition.
# Currently we dont have e2fsck compiled. So fs check would failed.

#<src>                                                 <mnt_point>               <type>        <mnt_flags and options>                                                                                                       <fs_mgr_flags>
/system                   erofs     system                               flags=display=system;logical;slotselect
/vendor                   erofs     vendor                               flags=display=vendor;logical;slotselect
/product                  erofs     product                              flags=display=product;logical;slotselect
/system_ext               erofs     system_ext                           flags=display=system_ext;logical;slotselect

/dev/block/by-name/userdata                            /data                     f2fs            noatime,nosuid,nodev,discard,noflush_merge,fsync_mode=nobarrier,reserve_root=134217,resgid=1065,inlinecrypt                  wait,check,formattable,quota,latemount,resize,reservedsize=128m,checkpoint=fs,fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized,keydirectory=/metadata/vold/metadata_encryption,fsverity
/dev/block/by-name/metadata                            /metadata                 ext4            noatime,nosuid,nodev,discard wait,check,formattable,first_stage_mount                                                        wait,check,formattable,quota,latemount,resize,reservedsize=128m,checkpoint=fs,fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized,keydirectory=/metadata/vold/metadata_encryption,fsverity

/dev/block/by-name/misc                                /misc                     emmc            defaults                                                                                                                     defaults

/dev/block/by-name/boot            /boot                     emmc            first_stage_mount,nofail,slotselect                                                                                          first_stage_mount,nofail,slotselect
